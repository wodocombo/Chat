<form class="channel-form" id="channel-add-form">
    <input class="channel-form-text" id="channel-add-form-text" type="text" name="channelName" placeholder="Имя для нового канала" />
    <button class="channel-sumbit" id="channel-add-form-sumbit" type="submit" value="Отправить"><i class="fa-solid fa-check" style="color: #ffffff;"></i></button>
